{
    "name": "blackjack-sim",
    "version": "1.0.0",
    "description": "Black Jack Simulations",
    "main": "root.js",
    "scripts": {
        "serve": "electron .",
        "build": "webpack --watch --mode development",
        "start": "npm-run-all --parallel build serve",
        "check-types": "tsc",
        "test": "jest --verbose",
        "test:debug": "jest --debug",
        "test:watch": "jest --watch",
        "test:coverage": "npx mkdirp reports/test-reports && cross-env TEST_REPORT_PATH=reports/test-reports TEST_REPORT_FILENAME=junit.xml jest --coverage --ci --testResultsProcessor='./node_modules/jest-junit-reporter'"
    },
    "author": "Frank Hu",
    "license": "ISC",
    "devDependencies": {
        "@babel/cli": "^7.1.5",
        "@babel/core": "^7.1.6",
        "@babel/plugin-proposal-class-properties": "^7.1.0",
        "@babel/plugin-proposal-decorators": "^7.1.6",
        "@babel/plugin-proposal-do-expressions": "^7.0.0",
        "@babel/plugin-proposal-export-default-from": "^7.0.0",
        "@babel/plugin-proposal-export-namespace-from": "^7.0.0",
        "@babel/plugin-proposal-function-bind": "^7.0.0",
        "@babel/plugin-proposal-function-sent": "^7.1.0",
        "@babel/plugin-proposal-json-strings": "^7.0.0",
        "@babel/plugin-proposal-logical-assignment-operators": "^7.0.0",
        "@babel/plugin-proposal-nullish-coalescing-operator": "^7.0.0",
        "@babel/plugin-proposal-numeric-separator": "^7.0.0",
        "@babel/plugin-proposal-object-rest-spread": "^7.0.0",
        "@babel/plugin-proposal-optional-chaining": "^7.0.0",
        "@babel/plugin-proposal-pipeline-operator": "^7.0.0",
        "@babel/plugin-proposal-throw-expressions": "^7.0.0",
        "@babel/plugin-syntax-dynamic-import": "^7.0.0",
        "@babel/plugin-syntax-import-meta": "^7.0.0",
        "@babel/polyfill": "^7.8.3",
        "@babel/preset-env": "^7.1.6",
        "@babel/preset-react": "^7.0.0",
        "@babel/preset-typescript": "^7.1.0",
        "@babel/register": "^7.0.0",
        "@types/jest": "^25.2.1",
        "@types/node": "^13.11.0",
        "@types/react": "^16.7.7",
        "@types/react-dom": "^16.0.10",
        "@types/sinon": "^7.0.3",
        "@types/styled-components": "^4.1.1",
        "babel-loader": "^8.0.4",
        "cross-env": "^7.0.2",
        "css-loader": "^1.0.1",
        "electron": "^3.0.10",
        "electron-reload": "^1.3.0",
        "enzyme": "^3.11.0",
        "enzyme-adapter-react-16": "^1.15.2",
        "enzyme-to-json": "^3.4.4",
        "event-stream": "^3.3.4",
        "extract-text-webpack-plugin": "^4.0.0-beta.0",
        "file-loader": "^2.0.0",
        "husky": "^4.2.3",
        "jest": "^25.2.7",
        "jest-junit-reporter": "^1.1.0",
        "mkdirp": "^1.0.4",
        "npm-run-all": "^4.1.3",
        "polish": "^0.2.3",
        "prettier": "^2.0.2",
        "react-hot-loader": "^4.3.12",
        "sinon": "^9.0.1",
        "styled-components": "^4.1.1",
        "ts-jest": "^25.3.1",
        "ts-node": "^7.0.1",
        "typescript": "^3.1.6",
        "webpack": "^4.26.0",
        "webpack-cli": "^3.1.2"
    },
    "dependencies": {
        "react": "^16.6.3",
        "react-dom": "^16.6.3"
    },
    "husky": {
        "hooks": {
            "pre-commit": "npm test",
            "pre-push": "npm run test:coverage"
        }
    }
}
